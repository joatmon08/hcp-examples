[
  {
    "portMappings": [
      {
        "hostPort": 8301,
        "protocol": "tcp",
        "containerPort": 8301
      },
      {
        "hostPort": 8301,
        "protocol": "udp",
        "containerPort": 8301
      },
      {
        "hostPort": 8302,
        "protocol": "tcp",
        "containerPort": 8302
      },
      {
        "hostPort": 8300,
        "protocol": "tcp",
        "containerPort": 8300
      },
      {
        "hostPort": 8600,
        "protocol": "tcp",
        "containerPort": 8600
      },
      {
        "hostPort": 8600,
        "protocol": "udp",
        "containerPort": 8600
      },
      {
        "hostPort": 8501,
        "protocol": "tcp",
        "containerPort": 8501
      },
      {
        "hostPort": 8502,
        "protocol": "tcp",
        "containerPort": 8502
      }
    ],
    "cpu": 10,
    "environment": [
      {
        "name": "CONSUL_DATACENTER",
        "value": "${consul_datacenter}"
      },
      {
        "name": "CONSUL_CLIENT",
        "value": "true"
      }
    ],
    "secrets": [
      {
        "valueFrom": "${consul_client_secret_arn}:retry_join::",
        "name": "CONSUL_HTTP_ADDR"
      },
      {
        "valueFrom": "${consul_client_secret_arn}:token::",
        "name": "CONSUL_HTTP_TOKEN"
      },
      {
        "valueFrom": "${consul_client_secret_arn}:certificate::",
        "name": "CONSUL_CA_PEM"
      },
      {
        "valueFrom": "${consul_client_secret_arn}:encrypt_key::",
        "name": "CONSUL_GOSSIP_ENCRYPT"
      }
    ],
    "memory": 100,
    "image": "${consul_image}",
    "name": "consul-client"
  }
]